{
    "title": {
        "en": "Pie Chart",
        "de": "Kreisdiagramm",
        "fr": "Diagramme temporel"
    },
    "extends": "raphael-chart",
    "author": {
        "name": "gka",
        "email": "gka@vis4.net"
    },
    "license": {
        "name": "MIT",
        "url": "http://www.opensource.org/licenses/mit-license.php"
    },
    "dimensions": 1,
    "options": {
        "pie-type": {
            "type": "radio",
            "label": {
                "en": "Type",
                "de": "Typ",
                "fr": "Type"
            },
            "options": [{
                "value": "full",
                "label": {
                    "en": "full circle",
                    "de": "gestapelt",
                    "fr": "linéaire"
                }
            },{
                "value": "horizontal",
                "label": {
                    "en": "donut",
                    "de": "normal",
                    "fr": "linéaire"
                }
            }],
            "default": "vertical"
        },
        "sort-values": {
            "type": "checkbox",
            "label": {
                "en": "Autmatically sort bars",
                "de": "Balken sortieren",
                "fr": "Type"
            },
            "default": true
        }
    },
    "libraries": [],
    "about": "http://site.com/understanding-bar-charts/",
    "compatibility": {
        "msie": "9+",
        "moz": "5+",
        "webkit": "3+",
        "ios": "no"
    }
}